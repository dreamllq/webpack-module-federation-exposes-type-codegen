{
  "name": "commander-ts-cli-template",
  "description": "模板",
  "version": "0.0.0",
  "files": [
    "/bin",
    "/lib"
  ],
  "bin": {
    "cli": "./bin/cli"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig-build.json && tsc-alias -p tsconfig-build.json",
    "preversion": "npm run build && npm run test",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "prepare": "husky install",
    "publish:next": "npm publish --registry=https://registry.npmjs.org/ --tag=next",
    "publish:beta": "npm publish --registry=https://registry.npmjs.org/ --tag=beta",
    "publish:latest": "npm publish --registry=https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {},
  "author": {
    "name": "lvliqi",
    "email": "lvliqi_d@163.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "dependencies": {
    "commander": "^12.0.0"
  }
}